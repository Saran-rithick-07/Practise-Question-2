package primenum;

public class Primenum {
	public static void main(String[] args){
		
		int num=7;
		int count=0;
		for(int i=1; i<=num; i++) {
			if(num%i==0) {
				count++;
			}
		}
	
		if(count==2){
			System.out.println("The Given Number is a Prime number");
		}else {
			System.out.println("The Given Number is not a Prime number");
			
		}
	
	}

}
